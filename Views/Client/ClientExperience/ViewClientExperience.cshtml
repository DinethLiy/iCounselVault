@{
    ViewData["Title"] = "Your Past Experiences";
    Layout = "~/Views/Shared/_ClientLayout.cshtml";
    dynamic? clientExperience = TempData["clientExperience"];
}
<body>
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Your Past Experiences</h1>
    </div>

    <!-- Profile Card -->
    <div class="col">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Experience Details</h6>
            </div>
            <div class="card-body">
                <div class="mb-3"><label class="form-label">School Experience: @clientExperience.SCHOOL_EXPERIENCE</label></div>
                <div class="mb-3"><label class="form-label">Higher Education: @clientExperience.HIGHER_EDU_EXPERIENCE</label></div>
                <div class="mb-3"><label class="form-label">Career: @clientExperience.JOB_EXPERIENCE</label></div>
                <div class="mb-3"><label class="form-label">Preferred Career Field: @clientExperience.PREFERED_CAREER_FIELD</label></div>
                <div class="mb-3">
                    <a class="btn btn-info btn-icon-split" asp-action="ShowEditClientExperience" asp-controller="ClientExperience">
                        <span class="text">Edit</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</body>
 